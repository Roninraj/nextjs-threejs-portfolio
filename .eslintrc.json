{
    "root": true,
    "extends": "next",
    "rules": {
        "react/no-unescaped-entities": "off",
        "@next/next/no-html-link-for-pages" : "off",
        "react/jsx-no-target-blank" : "off",
        "@next/next/inline-script-id" : "off",
        "@next/next/link-passhref" : "off",
        "@next/next/no-img-element" : "off",
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "^_",
                "varsIgnorePattern": "^_"
            }
        ]
    }
}